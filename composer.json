{
  "name": "rokde/baum",
  "type": "library",
  "description": "Baum is an implementation of the Nested Set pattern for Eloquent models.",
  "keywords": [
    "nested set",
    "laravel",
    "eloquent",
    "database"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Estanislau Trepat",
      "email": "estanis@etrepat.com",
      "homepage": "http://etrepat.com"
    },
    {
      "name": "Gary Pearman",
      "email": "gaz@red-robot.co.uk",
      "homepage": "http://red-robot.co.uk"
    },
    {
      "name": "Martin Seysen",
      "email": "mse@dogado.de",
      "homepage": "https://www.dogado.de"
    },
    {
      "name": "Robert Kummer",
      "email": "rok@ipunkt.biz",
      "homepage": "https://www.ipunkt.biz"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/console": "7.*|8.*",
    "illuminate/database": "7.*|8.*",
    "illuminate/events": "7.*|8.*",
    "illuminate/filesystem": "7.*|8.*",
    "illuminate/support": "7.*|8.*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "mockery/mockery": "~1.3"
  },
  "autoload": {
    "psr-4": {
      "Baum\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Baum\\Providers\\BaumServiceProvider"
      ]
    }
  },
    "replace": {
        "baum/baum": "2.0.*",
        "dogadogmbh/baum": "1.4.*",
        "gazsp/baum": "2.0.*"
    }
}
